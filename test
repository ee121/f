
function data($link2,$data2,$ua){
$curl = curl_init();
curl_setopt($curl, CURLOPT_URL, $link2);
curl_setopt($curl, CURLOPT_PORT, 443);
curl_setopt($curl, CURLOPT_RETURNTRANSFER,true);
curl_setopt($curl, CURLOPT_POST, 1);
curl_setopt($curl, CURLOPT_POSTFIELDS, $data2);
curl_setopt($curl, CURLOPT_HTTPHEADER, $ua);
$res7 = curl_exec($curl);
$err = curl_error($curl);
return $res7;
}
